[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_render/layer_1="Background"
2d_render/layer_2="Cities"
2d_render/layer_3="Connections"
2d_render/layer_4="UI"
2d_render/layer_5="Battle"
2d_render/layer_6="Effects"
2d_render/layer_7="Debug"

[logging]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/godot.log"
file_logging/max_log_files=5

[physics]

2d/run_on_separate_thread=true
common/enable_pause_aware_picking=true

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
renderer/rendering_method.mobile="gl_compatibility"
anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/msaa_3d=1
environment/defaults/default_clear_color=Color(0.15, 0.15, 0.2, 1)
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true

[threading]

worker_pool/max_threads=4

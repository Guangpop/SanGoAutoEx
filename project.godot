; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="三國天命放置小遊戲"
config/description="A Three Kingdoms idle strategy game with mobile-first design"
config/version="1.0"
run/main_scene="res://scenes/main/Main_Mobile.tscn"
config/features=PackedStringArray("4.5")
run/low_processor_mode=true

[autoload]

EventBus="*res://scripts/core/EventBus.gd"
GameStateManager="*res://scripts/core/GameStateManager.gd"
LogManager="*res://scripts/core/LogManager.gd"
DataManager="*res://scripts/data/DataManager.gd"
GameCore="*res://scripts/core/GameCore.gd"
SaveManager="*res://scripts/systems/SaveManager.gd"
AudioManager="*res://scripts/systems/AudioManager.gd"
SkillSelectionManager="*res://scripts/systems/SkillSelectionManager.gd"
BattleManager="*res://scripts/systems/BattleManager.gd"
CityManager="*res://scripts/systems/CityManager.gd"
AutoBattleManager="*res://scripts/systems/AutoBattleManager.gd"
GameEventManager="*res://scripts/systems/GameEventManager.gd"
BattleAutomationController="*res://scripts/systems/BattleAutomationController.gd"
GeneralsManager="*res://scripts/systems/GeneralsManager.gd"
NationManager="*res://scripts/systems/NationManager.gd"
RandomEventSystem="*res://scripts/systems/RandomEventSystem.gd"

[debug]

gdscript/warnings/unused_variable=true
gdscript/warnings/unused_local_constant=true
gdscript/warnings/unused_private_class_variable=true
gdscript/warnings/unused_parameter=true
gdscript/warnings/unused_signal=true
gdscript/warnings/shadowed_variable=true
gdscript/warnings/unreachable_code=true
gdscript/warnings/standalone_expression=true
gdscript/warnings/incompatible_ternary=true
gdscript/warnings/unsafe_property_access=true
gdscript/warnings/unsafe_method_access=true
gdscript/warnings/unsafe_cast=true
gdscript/warnings/unsafe_call_argument=true
gdscript/warnings/narrowing_conversion=true

[display]

window/size/viewport_width=414
window/size/viewport_height=896
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "blue",
"res://data/": "green",
"res://docs/": "yellow",
"res://logs/": "red",
"res://scenes/": "orange",
"res://scripts/": "purple",
"res://tests/": "pink"
}

[gui]

theme/custom="res://assets/ui/main_theme.tres"

